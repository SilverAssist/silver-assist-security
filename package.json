{
  "name": "silver-assist-security",
  "version": "1.1.15",
  "description": "WordPress security plugin that resolves critical security vulnerabilities found in WordPress security audits",
  "main": "silver-assist-security.php",
  "scripts": {
    "grunt": "grunt",
    "minify": "npm run minify:css && npm run minify:js",
    "minify:css": "postcss assets/css/*.css --base assets/css --dir assets/css --ext .min.css",
    "minify:js": "grunt uglify",
    "build": "npm run clean && npm run minify",
    "clean": "rm -f assets/css/*.min.css assets/js/*.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SilverAssist/silver-assist-security.git"
  },
  "keywords": [
    "wordpress",
    "security",
    "plugin",
    "brute-force",
    "graphql",
    "httponly",
    "cookies"
  ],
  "author": "Silver Assist Security Team",
  "license": "PolyForm-Noncommercial-1.0.0",
  "bugs": {
    "url": "https://github.com/SilverAssist/silver-assist-security/issues"
  },
  "homepage": "https://github.com/SilverAssist/silver-assist-security#readme",
  "devDependencies": {
    "cssnano": "^7.1.2",
    "grunt": "^1.6.1",
    "grunt-contrib-uglify": "^5.2.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
